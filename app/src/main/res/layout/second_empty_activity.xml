<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/plainText"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondEmptyActivity">


    <Button
        android:id="@+id/buttonPreviousActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="40dp"
        android:text="1st Screen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <fragment
        android:id="@+id/fragment_cat"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.example.myapplication2.CatFragment"
        tools:layout="@layout/fragment_cat" />

</androidx.constraintlayout.widget.ConstraintLayout>